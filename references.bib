}

@article{macnealePesticidesAquaticFood2010,
  title = {Pesticides, Aquatic Food Webs, and the Conservation of {{Pacific}} Salmon},
  author = {Macneale, Kate H and Kiffney, Peter M and Scholz, Nathaniel L},
